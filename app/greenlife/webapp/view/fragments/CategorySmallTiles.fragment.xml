<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout">
    <GenericTile id="dangerous" class="smallTiles" header="{i18n>dangerous}" press="chooseCategory" backgroundImage="{pictureModel>/backgroundPicture}">
        <TileContent>
            <ImageContent src="sap-icon://solid-icons/spray-can"/>
        </TileContent>
    </GenericTile>
    <GenericTile id="automoto" class="smallTiles" header="{i18n>automoto}" press="chooseCategory" backgroundImage="{pictureModel>/backgroundPicture}">
        <TileContent>
            <ImageContent src="sap-icon://solid-icons/car"/>
        </TileContent>
    </GenericTile>
    <GenericTile id="constructions" class="smallTiles" header="{i18n>constructions}" press="chooseCategory" backgroundImage="{pictureModel>/backgroundPicture}">
        <TileContent>
            <ImageContent src="sap-icon://solid-icons/person-digging"/>
        </TileContent>
    </GenericTile>
    <GenericTile id="wood" class="smallTiles" header="{i18n>wood}" press="chooseCategory" backgroundImage="{pictureModel>/backgroundPicture}">
        <TileContent>
            <ImageContent src="sap-icon://solid-icons/tree"/>
        </TileContent>
    </GenericTile>
    <GenericTile id="textile" class="smallTiles" header="{i18n>textile}" press="chooseCategory" backgroundImage="{pictureModel>/backgroundPicture}">
        <TileContent>
            <ImageContent src="sap-icon://solid-icons/shirt"/>
        </TileContent>
    </GenericTile>
    <GenericTile id="others" class="smallTiles" header="{i18n>others}" press="chooseCategory" backgroundImage="{pictureModel>/backgroundPicture}">
        <TileContent>
            <ImageContent src="sap-icon://solid-icons/robot"/>
        </TileContent>
    </GenericTile>
</core:FragmentDefinition>