<core:FragmentDefinition xmlns="sap.m"
    xmlns:vk="sap.ui.vk"
    xmlns:l="sap.ui.layout"
    xmlns:vbm="sap.ui.vbm"
    xmlns:core="sap.ui.core">
    <Dialog resizable="true" stretch="true" horizontalScrolling="false" verticalScrolling="false">
        <l:FixFlex class="fixFlexVertical">
            <l:fixContent>
            </l:fixContent>
            <l:flexContent >
                <vbm:GeoMap id="map" centerPosition="{mapModel>/center}" initialZoom="{mapModel>/initialZoom}" width="100%" height="100%" mapConfiguration="{mapConfigModel>/config}" click="pressMap">
                    <vbm:vos>
                        <vbm:Spots items="{mapPointModel>/}">
                            <vbm:items>
                                <vbm:Spot position="{mapPointModel>location}">
                                    <vbm:customData>
                                        <core:CustomData key='status' />
                                    </vbm:customData>
                                </vbm:Spot>
                            </vbm:items>
                        </vbm:Spots>
                    </vbm:vos>
                </vbm:GeoMap>
            </l:flexContent>
        </l:FixFlex>
    </Dialog>
</core:FragmentDefinition>